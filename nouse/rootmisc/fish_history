- cmd: co shared-data/
- cmd: cd shared-data/
- cmd: co -R shared-data/
- cmd: rsync -a /mnt/shared-data/phone/ /mnt/data/phone
    - /mnt/data/phone
- cmd: cd ../kanshi/
- cmd: n kanshi/config
    - kanshi/config
- cmd: ccd
- cmd: mv phonetoshared.sh phonebup.sh
- cmd: n pctoshared.sh
    - pctoshared.sh
- cmd: n sharedtopc.sh
    - sharedtopc.sh
- cmd: n phonebup.sh
    - phonebup.sh
- cmd: time
- cmd: ./pctoshared.sh
- cmd: ./sharedtopc.sh
- cmd: s reflector --country Israel --latest 5 --sort rate --protocol https --verbose
- cmd: s reflector --verbose -l 200 -n 20 -p http --sort rate
- cmd: s reflector --verbose -l 200 -n 20 -p https --sort rate
    - /etc/xdg/reflector/reflector.conf
- cmd: s reflector --verbose -l 200 -n 20 -p https --sort rate --latest 5
- cmd: s reflector --verbose -p https --sort rate --latest 5
- cmd: s reflector --verbose -p https --sort rate --latest 10
